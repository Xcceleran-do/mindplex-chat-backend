{
    "containerDefinitions": [
        {
            "name": "web",
            "image": "521520215337.dkr.ecr.us-east-1.amazonaws.com/stagingchat",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "web-9010-tcp",
                    "containerPort": 9010,
                    "hostPort": 9010,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "command": [
                "bash",
                "-c",
                "fastapi dev ./src/main.py --port 9010 --host 0.0.0.0"
            ],
            "environment": [
                {
                    "name": "MINDPLEX_URL",
                    "value": "REPLACE_ME_MINDPLEX_URL"
                },
                {
                    "name":"POSTGRES_USER",
                    "value":"REPLACE_ME_POSTGRES_USER"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "REPLACE_ME_POSTGRES_PASSWORD"
                },
                {
                    "name": "POSTGRES_DB",
                    "value": "REPLACE_ME_POSTGRES_DB"
                },
                {
                    "name": "POSTGRES_HOST",
                    "value": "REPLACE_ME_POSTGRES_HOST"
                },
                {
                    "name": "POSTGRES_PORT",
                    "value": "REPLACE_ME_POSTGRES_PORT"
                },
                {
                    "name": "ENVIRONMENT",
                    "value": "REPLACE_ME_ENVIRONMENT"
                },
                {
                    "name": "PGADMIN_DEFAULT_EMAIL",
                    "value": "REPLACE_ME_PGADMIN_DEFAULT_EMAIL"
                },
                {
                    "name": "PGADMIN_DEFAULT_PASSWORD",
                    "value": "REPLACE_ME_PGADMIN_DEFAULT_PASSWORD"
                },
                {
                    "name": "PUBLIC_CHAT_BACKEND_HOST",
                    "value": "REPLACE_ME_PUBLIC_CHAT_BACKEND_HOST"
                },
                {
                    "name": "AXIOM_TOKEN",
                    "value": "REPLACE_ME_AXIOM_TOKEN"
                },
                {
                    "name": "AXIOM_DATASET",
                    "value": "REPLACE_ME_AXIOM_DATASET"
                }
            ],
          "mountPoints": [],
          "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                "awslogs-group": "/ecs/staging-chat/web",
                "awslogs-region": "us-east-1",
                "awslogs-stream-prefix": "redis"
                }
            },
          "volumesFrom": [],
          "systemControls": []
        },
        {
            "name": "zookeeper",
            "image": "confluentinc/cp-zookeeper:7.9.0",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "zookeeper-2181-tcp",
                    "containerPort": 2181,
                    "hostPort": 2181,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": false,
            "environment": [
                {
                    "name": "ZOOKEEPER_CLIENT_PORT",
                    "value": "2181"
                },
                {
                    "name":"ZOOKEEPER_TICK_TIME",
                    "value":"2000"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                    "logDriver": "awslogs",
                    "options": {
                    "awslogs-group": "/ecs/staging-chat/zookeeper",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "redis"
                    }
                },
            "systemControls": []
        },
        {
            "name": "kafka",
            "image": "confluentinc/cp-kafka:7.9.0",
            "portMappings": [
                {
                    "name": "kafka-9092-tcp",
                    "containerPort": 9092,
                    "hostPort": 9092,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": false,
            "environment": [
                {
                    "name": "KAFKA_BROKER_ID",
                    "value": "1"
                },
                {
                    "name":"KAFKA_ZOOKEEPER_CONNECT",
                    "value":"2181"
                },
                {
                    "name": "KAFKA_ADVERTISED_LISTENERS",
                    "value": "PLAINTEXT://kafka:9092"
                },
                {
                    "name": "KAFKA_OFFSETS_TOPIC_REPLICATION_FACTOR",
                    "value": "1"
                }
            ],
            "mountPoints": [],
            "logConfiguration": {
                    "logDriver": "awslogs",
                    "options": {
                    "awslogs-group": "/ecs/staging-chat/kafka",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "redis"
                    }
                },
            "volumesFrom": [],
            "dependsOn": [
                {
                    "containerName": "zookeeper",
                    "condition": "START"
                }
            ],
            "systemControls": []
        }
  ],
  "family": "stagingchat",
  "taskRoleArn": "arn:aws:iam::521520215337:role/FileSystemAccess",
  "executionRoleArn": "arn:aws:iam::521520215337:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "512",
  "memory": "2048",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  },
  "enableFaultInjection": false,
  "tags": []
}